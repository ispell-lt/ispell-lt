        #J A S          >       -AS,O           # vëjo, galvijo
        #J A S          >       -AS,UI
        #J A S          >       -AS,À
        #J A S          >       -AS,U
        J A S           >       -AS,UJE         # vëjuje/vëjyje
        J A S           >       -AS,YJE
        J A S           >       -S,U

        # dgsk., vyr. gim.
        #I A S          >       -S,I            # elniai, keliai
        #I A S          >       -AS,Ø
        #I A S          >       -S,MS
        #I A S          >       -AS,US
        #I A S          >       -S,IS
